{
  "name": "neoway-components",
  "description": "A collection of Lit web components",
  "version": "1.0.3",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components/accessible-modal": {
      "import": "./dist/AccessibleModal/index.js",
      "require": "./dist/AccessibleModal/index.d.ts"
    },
    "./components/search-input": {
      "import": "./dist/SearchInput/index.js",
      "require": "./dist/SearchInput/index.d.ts"
    },
    "./components/virtualized-table": {
      "import": "./dist/VirtualizedTable/index.js",
      "require": "./dist/VirtualizedTable/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && rollup -c",
    "preview": "vite preview",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint \"**/*.ts\" --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepublish-only": "npm run build",
    "test": "web-test-runner",
    "test:watch": "web-test-runner --watch",
    "test:coverage": "web-test-runner --coverage",
    "test:debug": "web-test-runner --open --watch"
  },
  "keywords": [
    "lit",
    "web-components",
    "components"
  ],
  "author": "Lucas Katayama",
  "license": "MIT",
  "peerDependencies": {
    "lit": "^3.3.1"
  },
  "dependencies": {
    "@lit-labs/virtualizer": "^2.1.1",
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/css": "^0.13.0",
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@open-wc/testing": "^4.0.0",
    "@remcovaes/web-test-runner-vite-plugin": "^1.3.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/web-components-vite": "^9.1.10",
    "@types/jest": "^30.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@web/dev-server-esbuild": "^1.0.4",
    "@web/dev-server-rollup": "^0.6.4",
    "@web/test-runner": "^0.20.2",
    "eslint": "^9.37.0",
    "eslint-plugin-storybook": "^9.1.10",
    "git-commit-msg-linter": "^5.0.8",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "playwright": "^1.56.0",
    "rollup": "^4.52.4",
    "rollup-plugin-dts": "^6.2.3",
    "sinon": "^21.0.0",
    "storybook": "^9.1.10",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
